{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "rules": {
        // overrides
        "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
        "import/extensions": ["error", "never", { "packages": "always" }],
    }
}
